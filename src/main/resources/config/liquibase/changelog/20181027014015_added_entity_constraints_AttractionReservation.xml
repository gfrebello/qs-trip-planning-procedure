<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity AttractionReservation.
    -->
    <changeSet id="20181027014015-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="attraction_id"
                                 baseTableName="attraction_reservation"
                                 constraintName="fk_attraction_reservation_attraction_id"
                                 referencedColumnNames="id"
                                 referencedTableName="attraction"/>

        <addForeignKeyConstraint baseColumnNames="trip_id"
                                 baseTableName="attraction_reservation"
                                 constraintName="fk_attraction_reservation_trip_id"
                                 referencedColumnNames="id"
                                 referencedTableName="trip"/>

    </changeSet>
</databaseChangeLog>
